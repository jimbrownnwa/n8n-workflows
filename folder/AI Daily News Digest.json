    {
      "parameters": {
        "jsCode": "// Parse the AI response\nconst aiResponse = JSON.parse($input.first().json.message.content);\nconst today = new Date().toLocaleDateString('en-US', { \n  weekday: 'long', \n  year: 'numeric', \n  month: 'long', \n  day: 'numeric' \n});\n\n// Build the email content\nlet emailBody = `<h1>${today}: Daily AI News Digest</h1>\\n\\n`;\nemailBody += `<h2>ðŸ“° Quick hits on the ${aiResponse.topStories.length} most interesting stories for Acme Corp:</h2>\\n<ul>\\n`;\n\n// Add top stories with blurbs\nfor (const story of aiResponse.topStories) {\n  emailBody += `<li><strong>${story.title}</strong> <a href=\"${story.link}\">[Link]</a> (${story.source})<br/>\\n`;\n  emailBody += `<em>${story.blurb}</em></li>\\n\\n`;\n}\n\nemailBody += `</ul>\\n\\n`;\n\n// Add other articles if any\nif (aiResponse.otherArticles && aiResponse.otherArticles.length > 0) {\n  emailBody += `<h2>ðŸ“š Other Relevant Articles:</h2>\\n<ul>\\n`;\n  \n  for (const article of aiResponse.otherArticles) {\n    emailBody += `<li><strong>${article.title}</strong> <a href=\"${article.link}\">[Link]</a> (${article.source})</li>\\n`;\n  }\n  \n  emailBody += `</ul>\\n\\n`;\n}\n\n// Add footer\nemailBody += `<hr/>\\n<p><small>Filtered from ${$('Prepare for AI Analysis').first().json.article_count} articles | `;\nemailBody += `${aiResponse.rejectedCount || 0} not relevant | Automated by n8n</small></p>`;\n\nreturn {\n  json: {\n    subject: `${today}: Daily AI News Digest`,\n    body: emailBody,\n    topStoriesCount: aiResponse.topStories.length,\n    otherArticlesCount: aiResponse.otherArticles?.length || 0,\n    totalProcessed: $('Prepare for AI Analysis').first().json.article_count\n  }\n};"
      },

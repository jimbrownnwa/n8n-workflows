        "parameters": {
          "jsCode": "// Get the error data\nconst errorData = $input.first().json;\n\n// Format timestamp to be more readable\nconst timestamp = new Date(errorData.timestamp).toLocaleString('en-US', {\n  weekday: 'long',\n  year: 'numeric',\n  month: 'long',\n  day: 'numeric',\n  hour: '2-digit',\n  minute: '2-digit',\n  timeZone: 'America/Chicago'\n});\n\n// Build HTML email\nconst htmlBody = `\n<!DOCTYPE html>\n<html>\n<head>\n  <style>\n    body { font-family: Arial, sans-serif; line-height: 1.6; color: #333; }\n    .container { max-width: 600px; margin: 0 auto; padding: 20px; }\n    .alert-box { background: #fff5f5; border-left: 5px solid #e74c3c; padding: 20px; margin: 20px 0; border-radius: 4px; }\n    .alert-icon { font-size: 24px; margin-bottom: 10px; }\n    .intro { font-size: 16px; margin-bottom: 20px; }\n    .details { background: white; border: 1px solid #ddd; padding: 15px; border-radius: 4px; margin: 20px 0; }\n    .details h3 { margin-top: 0; color: #e74c3c; }\n    ul { padding-left: 20px; }\n    li { margin: 10px 0; }\n    .label { font-weight: bold; color: #555; }\n    .value { font-family: 'Courier New', monospace; background: #f5f5f5; padding: 2px 6px; border-radius: 3px; }\n    .footer { margin-top: 30px; padding-top: 20px; border-top: 1px solid #ddd; font-size: 12px; color: #666; }\n    .button { display: inline-block; background: #fff; color: black; padding: 10px 20px; text-decoration: none; border-radius: 4px; border-color: black; margin-top: 15px; }\n  </style>\n</head>\n<body>\n  <div class=\"container\">\n    <div class=\"alert-box\">\n      <div class=\"alert-icon\">ðŸš¨</div>\n      <h2 style=\"margin: 0 0 10px 0; color: #e74c3c;\">Workflow Execution Failed</h2>\n      <p class=\"intro\">\n        The <strong>${errorData.workflow_name}</strong> workflow encountered an error and was unable to complete successfully. \n        Please review the details below and take appropriate action.\n      </p>\n    </div>\n\n    <div class=\"details\">\n      <h3>Error Details</h3>\n      <ul>\n        <li>\n          <span class=\"label\">Workflow:</span> \n          <span class=\"value\">${errorData.workflow_name}</span>\n        </li>\n        <li>\n          <span class=\"label\">Failed Node:</span> \n          <span class=\"value\">${errorData.failed_node}</span>\n        </li>\n        <li>\n          <span class=\"label\">Error Message:</span> \n          <span class=\"value\">${errorData.error_message}</span>\n        </li>\n        <li>\n          <span class=\"label\">Timestamp:</span> \n          <span class=\"value\">${timestamp}</span>\n        </li>\n        <li>\n          <span class=\"label\">Execution ID:</span> \n          <span class=\"value\">${errorData.execution_id}</span>\n        </li>\n      </ul>\n    </div>\n\n    <div style=\"text-align: center;\">\n      <a href=\"${errorData.execution_url}\" class=\"button\">View Full Execution Details</a>\n    </div>\n\n    <div class=\"footer\">\n      <p><strong>Next Steps:</strong></p>\n      <ol>\n        <li>Click the button above to view complete execution logs</li>\n        <li>Verify all API credentials and connections are valid</li>\n        <li>Check for any external service disruptions</li>\n        <li>Review the failed node configuration</li>\n        <li>Contact support if the issue persists</li>\n      </ol>\n      <p style=\"margin-top: 20px;\">\n        This is an automated notification from your n8n workflow automation system.\n      </p>\n    </div>\n  </div>\n</body>\n</html>\n`;\n\n// Return formatted email data\nreturn {\n  json: {\n    subject: `ðŸš¨ ${errorData.workflow_name} Failed - ${new Date(errorData.timestamp).toLocaleDateString()}`,\n    htmlBody: htmlBody,\n    textBody: `Workflow Failed: ${errorData.workflow_name}\\n\\nFailed Node: ${errorData.failed_node}\\nError: ${errorData.error_message}\\nTime: ${timestamp}\\n\\nView details: ${errorData.execution_url}`\n  }\n};"
